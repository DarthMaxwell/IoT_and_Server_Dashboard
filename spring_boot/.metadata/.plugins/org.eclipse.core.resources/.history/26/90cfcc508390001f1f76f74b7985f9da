package com.personal.dashboard.services;

import java.io.IOException;
import java.net.InetAddress;
import java.net.UnknownHostException;

public class PingService {

	public String testping() throws UnknownHostException, IOException {
		InetAddress inet;
		
		inet = InetAddress.getByAddress(new byte[] { (byte) 192, (byte) 168, 86, (byte) 163});
		System.out.println("Sending Ping Request to " + inet);
	    System.out.println(inet.isReachable(5000) ? "Host is reachable" : "Host is NOT reachable");
	    return (inet.isReachable(5000) ? "Host reached" : "Didnt find");
	}
}
